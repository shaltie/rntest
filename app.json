{
  "expo": {
    "name": "Inmost Chat",
    "slug": "inmost-chat",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/nastolki.png",
    "splash": {
      "image": "./assets/nastolki.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fckinsky.inmostchat",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.fckinsky.inmostchat",
      "versionCode": 1,
      "permissions": [
        "VIBRATE"
      ]
    }
  }
}
